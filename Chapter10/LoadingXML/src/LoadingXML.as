package{  import flash.net.URLLoader;  import flash.net.URLRequest;	import flash.events.Event;  import flash.display.*;  import flash.system.Security;  import flash.system.SecurityPanel;  [SWF(width="640", height="512",   backgroundColor="#FFFFFF", frameRate="60")]  	public class LoadingXML extends Sprite	{ 				public function LoadingXML():void		{ 		  /*		  Warning: You may have to create a .cfg file to		  specify that the folder containing the SWF is		  trusted. Only a trusted SWF file will be able to		  load the xml file using URLLoader.		  Also, you can find out which security sandbox		  type the SWF file has with this line of code:		  		  trace(Security.sandboxType);		  		  */		  		  var xmlLoader:URLLoader = new URLLoader();       xmlLoader.addEventListener(Event.COMPLETE, fileLoadedHandler);       xmlLoader.load        (new URLRequest("../assets/xml/maps.xml"));            }    private function fileLoadedHandler(event:Event):void     {      var xmlData:XML = new XML(event.target.data);      trace(xmlData);		}	}}