//Note: The compiler will warn you of duplicate variable//definitions if you recompile this code, but you can//safely ignore those. I've included duplicate variable//names so that this code matches the text in Chapter 10package{  import flash.events.Event;  import flash.display.*;  [SWF(width="640", height="512",   backgroundColor="#FFFFFF", frameRate="60")]  	public class ChangingXML extends Sprite	{ 				public function ChangingXML():void		{ 		  var gameData:XML = <gameData></gameData>;		  	    //1. Add a new element called player	    	    trace("1. Add a new element called player" + "\n"); 	    gameData.player = <player></player>;	    trace(gameData);  	    	    trace("\n" + "---------------------------------" + "\n"); 	    	    //2. Add a text node to the new element	    	    trace("2. Add a text node to the new element" + "\n"); 	    gameData.player = "Player Name";	    trace(gameData);  	    	    //You can also create the element and text node together	    //at the same time, like this:	    //gameData.player = <player>Player Name</player>;	    	    	    trace("\n" + "---------------------------------" + "\n"); 	    	    //3. Use appendChild to add a new element	    	    trace("3. Use appendChild to add a new element" + "\n"); 	    gameData.appendChild(<score>50</score>);	    trace(gameData);  	    	    trace("\n" + "---------------------------------" + "\n"); 	    	    //4. Use prependChild to add a new element before an	    //exiting element	    	    trace("4. Use prependChild to add a new element before an exiting element" + "\n"); 	    gameData.prependChild(<gameName>Cosmic Fluff Bubbles</gameName>);	    trace(gameData);  	    	    trace("\n" + "---------------------------------" + "\n"); 	    	    //5. Use insertChildAfter to specify after which element	    //you want to add the new element	    	    trace("5. Use insertChildAfter to specify after which element you want to add the new element" + "\n"); 	    gameData.insertChildAfter(gameData.player, <location>Bangalore</location>);	    trace(gameData);  	    	    //insertChildBefore allows you add new elements	    //before other elements	    	    trace("\n" + "---------------------------------" + "\n"); 	    	    //6. Add a text node to the new element	    	    trace("6. Add a text node to the new element" + "\n"); 	    gameData.score.@bubblesPopped = 13;	    trace(gameData);  	    	    //You can also create the element and text node together	    //at the same time, like this:	    //gameData.player = <player>Player Name</player>;	    	    	    trace("\n" + "---------------------------------" + "\n"); 	    	    	    //7. Build the XML from existing variables	    	    trace("8. Build the XML from existing variables" + "\n"); 	    	    var gameName:String = "Cosmic Fluff Bubbles";		  var player:String = "Player Name";		  var location:String = "Bangalore";		  var score:uint = 50;		  		  var newXML:XML 		    = 		    <gameData>          <gameName>{gameName}</gameName>          <player>{player}</player>          <location>{location}</location>          <score>{score}</score>        </gameData>        ;        	    trace(newXML); 	    	    trace("\n" + "---------------------------------" + "\n"); 	    	   //8. Use an existing variable to create an	   //XML document with attributes	    	    trace("8. Use an existing variable to create an XML document with attributes" + "\n"); 	    	    var gameName:String = "Cosmic Fluff Bubbles";	    var timesPlayed:uint = 3;		  		  var newXML:XML 		    = 		    <gameData>          <gameName>{gameName}</gameName>          <player>{player}</player>          <location>{location}</location>          <score timesPlayed = {timesPlayed}>{score}</score>        </gameData>        ;        	    trace(newXML); 	    	    //insertChildBefore allows you add new elements	    //before other elements	    	     trace("\n" + "---------------------------------" + "\n"); 	    	    //10. Delete an element with the delete keyword	    	    trace("10. Delete an element with the delete keyword" + "\n"); 	    	    delete gameData.location;	    	    trace(gameData);  	    	    trace("\n" + "---------------------------------" + "\n");	    	    //11. Delete an attribute with the delete keyword	    	    trace("11. Delete an attribute with the delete keyword" + "\n"); 	    	    delete gameData.score.@bubblesPopped;	    	    trace(gameData);  	    	    trace("\n" + "---------------------------------" + "\n");	    	     //12. Delete a text node with the delete keyword	    	    trace("12. Delete a text node  with the delete keyword" + "\n"); 	    	    delete gameData.gameName.text()[0];	    	    trace(gameData);  	    	    trace("\n" + "---------------------------------" + "\n");	    	        }	}}