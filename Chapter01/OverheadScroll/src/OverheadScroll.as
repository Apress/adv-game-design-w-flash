package {	import utils.*;	import flash.events.Event;	import flash.display.MovieClip;	public class OverheadScroll extends MovieClip	{		private var _player:Player;		private var _forest:Forest;				public function OverheadScroll()		{			_player = new Player();			addChild(_player);			_player.setX = 275;			_player.setY = 200;						_forest = new Forest();			addChild(_forest);			//_forest.x = 100;			//_forest.y = 100;					addEventListener(Event.ENTER_FRAME,onEnterFrame);		}				private function onEnterFrame(event:Event):void		{			//1. Update the moving objects			_player.update();			_forest.update();						//2. Check collisions (like stage boundaries)			StageBoundaries.overheadScroll(_player, _forest, stage);						//3. Correct the player's position			_player.x = _player.xPos;			_player.y = _player.yPos;						//4. Scroll the forest			_forest.x = _forest.xPos;			_forest.y = _forest.yPos;		}	}}