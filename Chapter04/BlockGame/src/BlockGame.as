package{	import flash.events.Event;  import flash.display.Sprite;  [SWF(width="550", height="400",   backgroundColor="#FFFFFF", frameRate="60")]  	public class BlockGame extends Sprite	{  	private var _gameModel:GameModel   	  = new GameModel();  	private var _gameController:GameController   	  = new GameController(_gameModel, stage);		private var _gameView:GameView 		  = new GameView(_gameModel, _gameController, stage);				public function BlockGame():void		{ 			addChild(_gameView);		}	}}