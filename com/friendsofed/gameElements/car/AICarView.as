package com.friendsofed.gameElements.car{  import flash.display.*;  import flash.events.Event;	import flash.events.KeyboardEvent;	import flash.ui.Keyboard;	import com.friendsofed.gameElements.primitives.*;	public class AICarView extends Sprite	{		private var _model:Object;		private var _controller:Object;		private var _stage:Object;		public var carBitmapData:BitmapData 		  = new BitmapData(48, 48, true, 0);		public var carBitmap:Bitmap 		  = new Bitmap(carBitmapData);      		public function AICarView		  (		    model:AVerletModel, 		    stage:Object		  ):void		{			this._model = model;		  //this._controller = controller;		  this._stage = stage;		  		  this.x = _model.xPos + 24;			this.y = _model.yPos + 24;						this.addChild(carBitmap);			carBitmap.x = -24;			carBitmap.y = -24;		  		  _model.addEventListener(Event.CHANGE, changeHandler);		  		}		private function changeHandler(event:Event):void		{ 			this.x = _model.xPos + 24;			this.y = _model.yPos + 24;			this.rotation = _model.rotationValue;		}	}}