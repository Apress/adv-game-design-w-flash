package com.friendsofed.gameElements.lunarLander{	import flash.events.KeyboardEvent;	import flash.ui.Keyboard;	import com.friendsofed.gameElements.primitives.*;	public class LanderUIController 	{	  private const EASING:Number = 0.1;		private var _model:Object;		private var _stage:Object;		public function LanderUIController(model:AVerletModel):void 		{			_model = model;		}		internal function processKeyDown(event:KeyboardEvent):void		{		  _model.friction = 1;		  		  if(event.keyCode == Keyboard.LEFT)		  {		    _model.acceleration_X = -0.1;		  }		  if(event.keyCode == Keyboard.RIGHT)		  {		    _model.acceleration_X = 0.1;		  }		  if(event.keyCode == Keyboard.UP)		  {		    _model.acceleration_Y = -0.2;		    _model.thrusterFired = true;		  }		  if(event.keyCode == Keyboard.DOWN)		  {		    //Not used		  }		}				internal function processKeyUp(event:KeyboardEvent):void  	{  	  if(event.keyCode == Keyboard.LEFT  	  ||(event.keyCode == Keyboard.RIGHT))		  {		    _model.acceleration_X = 0;		  }		  if(event.keyCode == Keyboard.UP)		  {		    _model.acceleration_Y = 0;		    _model.thrusterFired = false;		  }  		_model.friction = _model.frictionConstant;		}	}}