package com.friendsofed.gameElements.primitives{  import flash.display.Shape;  	public class ParticleView extends AVerletView	{		public function ParticleView(model:ParticleModel):void		{		  super(model);		}		override protected function draw():void		{		  var shape:Shape = new Shape();			shape.graphics.beginFill(0x000000);			shape.graphics.drawCircle(0, 0, 3);			shape.graphics.endFill();			addChild(shape);		}	}}