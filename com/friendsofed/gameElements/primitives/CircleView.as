package com.friendsofed.gameElements.primitives{	import flash.display.Shape;	import flash.filters.*;	public class CircleView extends AVerletView	{		public function CircleView(circleModel:CircleModel):void		{		  super(circleModel);		  super.model = CircleModel(model);		}		override protected function draw():void		{		  var shape:Shape = new Shape();		  shape.graphics.lineStyle(1);			shape.graphics.beginFill(model.color);			shape.graphics.drawCircle(0, 0, model.radius);			shape.graphics.endFill();			addChild(shape);		}	}}