package com.friendsofed.gameElements.primitives{  import flash.display.Sprite;  import flash.events.Event;	import flash.events.KeyboardEvent;	import flash.ui.Keyboard;	import flash.events.MouseEvent;	public class UIMazeView extends Sprite	{		private var _model:Object;		private var _controller:Object;		private var _stage:Object;		public function UIMazeView		  (		    model:AVerletModel, 		    controller:UIMazeController, 		    stage:Object		  ):void		{			this._model = model;		  this._controller = controller;		  this._stage = stage;		 			_stage.addEventListener			  (KeyboardEvent.KEY_DOWN,keyDownHandler);			}				private function keyDownHandler(event:KeyboardEvent):void		{			_controller.processKeyDown(event);		}	}}