package com.friendsofed.gameElements.primitives{  import flash.display.Sprite;  	//Keyboard Events	import flash.events.Event;	import flash.events.KeyboardEvent;	import flash.ui.Keyboard;	import flash.events.MouseEvent;	public class UIView extends Sprite	{		private var _model:Object;		private var _controller:Object;		public function UIView(model:CircleModel, controller:KeyboardController, stage:Object):void		{			_model = model;		  _controller = controller;						stage.addEventListener(KeyboardEvent.KEY_DOWN,keyDownHandler);			stage.addEventListener(KeyboardEvent.KEY_UP,keyUpHandler);			stage.addEventListener(MouseEvent.MOUSE_DOWN, mouseDownHandler);		}		private function keyDownHandler(event:KeyboardEvent):void		{			_controller.processKeyDown(event);		}		private function keyUpHandler(event:KeyboardEvent):void		{			_controller.processKeyUp(event);		}		private function mouseDownHandler(event:Event):void  	{  		_controller.processMouseDown(event);  	}	}}