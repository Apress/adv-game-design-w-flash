package com.friendsofed.gameElements.primitives{	import flash.display.*;	import flash.filters.*;	public class CircleBlockView extends AVerletView	{		public function CircleBlockView(circleModel:CircleModel):void		{		  super(circleModel);		}		override protected function draw():void		{		  var shape:Shape = new Shape();		  shape.graphics.lineStyle(1);			shape.graphics.beginFill(model.color);			shape.graphics.drawCircle(0, 0, model.radius);			shape.graphics.endFill();						//Add a bevel and drop shadow filter			var filters:Array = new Array();			filters = shape.filters;			filters.push(new BevelFilter(2, 135, 0xFFFFFF, 0.50, 			                             0x000000, 0.50, 2, 2));			filters.push(new DropShadowFilter(2, 135, 0x000000, 			                                  0.35, 2, 2));			shape.filters = filters;						addChild(shape);		}	}}