package{	import flash.events.Event;  import flash.display.Sprite;  import com.friendsofed.utils.*;  import com.friendsofed.gameElements.primitives.*;  [SWF(width="550", height="400",   backgroundColor="#FFFFFF", frameRate="60")]  	public class Circle extends Sprite	{			//Create the circle and add its UI controller  		private var _c1:CircleModel   		  = new CircleModel(30);  		private var _UIController:UIController   		  = new UIController(_c1);  		private var _c1_View:CircleView   		  = new CircleView(_c1);  		private var _UIView:UIView   		  = new UIView(_c1, _UIController, stage);				public function Circle():void		{ 		  //Add the circle to the stage			addChild(_c1_View);			_c1.setX = 250;			_c1.setY = 200;						addEventListener(Event.ENTER_FRAME, enterFrameHandler);		}				private function enterFrameHandler(event:Event):void		{ 			//Update the circle			_c1.update();			StageBoundaries.wrap(_c1, stage);       		}	}}